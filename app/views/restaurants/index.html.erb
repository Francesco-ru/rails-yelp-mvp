
<div class='container'>
  <% @restaurants.each do |rest| %>
    <div class='restaurant'>
      <h1><%= link_to rest.name, restaurant_path(rest.id) %></h1>
      <p><%= rest.category %></p>
      <p><%= rest.address %></p>
    </div>
  <% end %>
  <%= link_to 'Add a restaurant', new_restaurant_path %>
</div>
